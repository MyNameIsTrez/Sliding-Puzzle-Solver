		{
			"name": "C++",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/puzzle",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"setupCommands": [
			  {
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			  }
			],
			"preLaunchTask": "C++ build",
			"internalConsoleOptions": "neverOpen"
		},
